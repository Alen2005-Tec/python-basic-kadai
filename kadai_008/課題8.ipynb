        "#ランダムな整数を利用するために、randamモジュールをインポート\n",
        "import random\n",
        "\n",
        "var = random.randint(1,100)\n",
        "print(var)\n",
        "\n",
        "if var % 3 == 0:\n",
        "  print(\"Fizz\")\n",
        "\n",
        "elif var % 5 == 0:\n",
        "  print(\"Buzz\")\n",
        "\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "  print(\"FizzBuzz\")"
      ]
